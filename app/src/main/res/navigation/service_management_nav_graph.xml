<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/service_management_nav_graph"
    app:startDestination="@id/servicesFragment">

    <fragment
        android:id="@+id/createNewServicesFragment"
        android:name="com.example.petpatandroidappdemo.ui.fragments.service_management.CreateNewServicesFragment"
        android:label="CreateNewServicesFragment"
        tools:layout="@layout/fragment_create_new_services" />
    <fragment
        android:id="@+id/addNewServicesFragment"
        android:name="com.example.petpatandroidappdemo.ui.fragments.service_management.AddNewServicesFragment"
        android:label="fragment_add_new_services"
        tools:layout="@layout/fragment_add_new_services" >
        <action
            android:id="@+id/action_addNewServicesFragment_to_createNewServicesFragment"
            app:destination="@id/createNewServicesFragment" />
    </fragment>
    <fragment
        android:id="@+id/servicesFragment"
        android:name="com.example.petpatandroidappdemo.ui.fragments.service_management.ServicesFragment"
        android:label="fragment_services"
        tools:layout="@layout/fragment_services" >
        <action
            android:id="@+id/action_servicesFragment_to_addNewServicesFragment"
            app:destination="@id/addNewServicesFragment" />
    </fragment>
</navigation>